#include<stdio.h>
main()
{
	int marks;
	printf("Enter the Marks:");
	scanf("%d",&marks);
	if(marks>=85)
	{
		printf("GRADE A");
	}
	else if(marks>=70)
	{
		printf("GRADE B");
	}
	else if(marks>=55)
	{
		printf("GRADE C");
	}
	else if(marks>=40)
	{
		printf("GRADE D");
	}
	else
	printf("GRADE F");
}
